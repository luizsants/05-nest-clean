{
  "extends": ["@rocketseat/eslint-config/node"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-console": "warn",
    "no-useless-constructor": "off",
    "no-unused-vars": "off",
    "no-void": "off",
    "no-new": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_"
      }
    ]
    // "@typescript-eslint/no-explicit-any": "off"
  }
}
